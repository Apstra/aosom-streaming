groups:
- name: aos_rates
  rules:
  - record: interface_counters_tx_bps
    expr: 8*interface_counters_tx_bytes/5
  - record: interface_counters_rx_bps
    expr: 8*interface_counters_rx_bytes/5
  - record: interface_counters_rx_unicast_pps
    expr: interface_counters_rx_unicast_packets/5
  - record: interface_counters_rx_broadcast_pps
    expr: interface_counters_rx_broadcast_packets/5
  - record: interface_counters_rx_multicast_pps
    expr: interface_counters_rx_multicast_packets/5
  - record: interface_counters_tx_unicast_pps
    expr: interface_counters_tx_unicast_packets/5
  - record: interface_counters_tx_broadcast_pps
    expr: interface_counters_tx_broadcast_packets/5
  - record: interface_counters_tx_multicast_pps
    expr: interface_counters_tx_multicast_packets/5
  - record: interface_counters_rx_discard_pps
    expr: interface_counters_rx_discard_packets/5
  - record: interface_counters_tx_discard_pps
    expr: interface_counters_tx_discard_packets/5
  - record: interface_counters_rx_error_pps
    expr: interface_counters_rx_error_packets/5
  - record: interface_counters_tx_error_pps
    expr: interface_counters_tx_error_packets/5

# alert_liveness_status_clean{actual_agents="", expected_agents=""} = alert_liveness_status

# interface_counters_total_pps = sum(interface_counters_tx_unicast_pps + interface_counters_tx_broadcast_pps + interface_counters_rx_unicast_pps + interface_counters_rx_broadcast_pps + interface_counters_rx_multicast_pps) by (device, role, blueprint)
